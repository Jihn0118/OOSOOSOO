<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_home">

    <keep_state_fragment
        android:id="@+id/navigation_home"
        android:name="kr.ac.kpu.oosoosoo.home.HomeFragment"
        android:label="@string/title_home" />

    <keep_state_fragment
        android:id="@+id/navigation_dashboard"
        android:name="kr.ac.kpu.oosoosoo.dashboard.DashboardFragment"
        android:label="@string/title_dashboard" />

    <keep_state_fragment
        android:id="@+id/navigation_trends"
        android:name="kr.ac.kpu.oosoosoo.recommends.TrendsFragment"
        android:label="@string/title_trends" />

    <keep_state_fragment
        android:id="@+id/navigation_setting"
        android:name="kr.ac.kpu.oosoosoo.setting.SettingFragment"
        android:label="@string/title_setting" />
</navigation>